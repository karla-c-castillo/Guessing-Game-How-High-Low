print("Guess the number!")
guess = input('')

while True:
  user_input = input("Enter num: ") # recv user str input
  user_input = int(user_input)
  if(user_input > number):
    print("Too high!")
    print("Incorrect guess. Try again!")
  if(user_input < number):
    print("Too low!")
    print("Incorrect guess. Try again!")
  if(user_input == number):
    print("You guessed right!")
    break

exit(0)